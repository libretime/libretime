    "libretime/celery-php": "dev-main",
    "libretime/propel1": "dev-main",
    "php-amqplib/php-amqplib": "^3.0",
    "simplepie/simplepie": "^1.5",
    "symfony/config": "^5.4",
    "zf1s/zend-acl": "^1.13",
    "zf1s/zend-application": "^1.13",
    "zf1s/zend-auth": "^1.13",
    "zf1s/zend-cache": "^1.13",
