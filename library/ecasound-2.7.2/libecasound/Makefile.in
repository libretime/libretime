	cd $(DESTDIR)$(includedir)/libecasound && \
	rm -f   $(ecasound_audiofx_include) \
		$(ecasound_preset_include) \
		$(ecasound_audioio_include) \
		$(ecasound_controller_include) \
		$(ecasound_midi_include) \
		$(ecasound_general_include)
